local function OpenDonationPage(ply, text, team )
	if string.sub( text, 1, 8 ) == "!w" then
		
			ply:SendLua( [[ gui.OpenURL( "https://steamcommunity.com/id/theazad/" ) ]] )
			
			end
end
hook.Add( "PlayerSay", "OpenDonationPage", OpenDonationPage )

